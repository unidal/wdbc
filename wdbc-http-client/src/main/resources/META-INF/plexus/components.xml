<plexus>
	<components>
		<component>
			<role>org.unidal.wdbc.http.Session</role>
			<implementation>org.unidal.wdbc.http.impl.DefaultSession</implementation>
			<configuration>
				<headers>
					<header name="User-Agent">Mozilla/4.0 (compatible; MSIE 6.0; Windows NT 5.1; SV1; Maxthon; .NET CLR 1.1.4322)</header>
				</headers>
			</configuration>
			<requirements>
				<requirement>
					<role>org.unidal.wdbc.http.cache.CacheManager</role>
				</requirement>
			</requirements>
		</component>
		<component>
			<role>org.unidal.wdbc.http.impl.FormRequest</role>
			<implementation>org.unidal.wdbc.http.impl.FormRequest</implementation>
			<instantiation-strategy>per-lookup</instantiation-strategy>
		</component>
		<component>
			<role>org.unidal.wdbc.http.Handler</role>
			<implementation>org.unidal.wdbc.http.impl.DefaultHandler</implementation>
