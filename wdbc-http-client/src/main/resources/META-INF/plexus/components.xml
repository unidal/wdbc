		</component>
		<component>
			<role>org.unidal.wdbc.http.cache.CacheManager</role>
			<implementation>org.unidal.wdbc.http.cache.NoCacheManager</implementation>
		</component>
		<component>
			<role>org.unidal.wdbc.http.Processor</role>
			<implementation>org.unidal.wdbc.http.impl.DefaultProcessor</implementation>
		</component>
